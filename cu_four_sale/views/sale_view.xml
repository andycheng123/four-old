            <field name="type">form</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <form string="發票資料">
                    <sheet>
                        <group>
                            <group col="2">
                                <field name="partner_id" invisible="1"/>>
                                <field name="name"/>
                                <field name="type_invoice"/>
